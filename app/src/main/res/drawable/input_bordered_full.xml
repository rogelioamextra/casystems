<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gray shadow. -->
    <item>
        <shape>
            <solid android:color="#717D7E" />
            <corners
                android:radius="90dp"/>
        </shape>
    </item>
    <!-- White foreground. -->
    <item

        >
        <shape>
            <solid android:color="#ffff" />
            <stroke
                android:width="1dp"
                android:color="#0000006A" />
            <corners
                android:radius="90dp"
                />
        </shape>
    </item>
</layer-list>