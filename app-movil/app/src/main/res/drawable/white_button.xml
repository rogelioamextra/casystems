<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gray shadow. -->
    <item
        android:left="0dp"
        android:top="3dp">
        <shape>
            <solid android:color="#717D7E" />
            <corners android:radius="320dp" />
        </shape>
    </item>
    <!-- White foreground. -->
    <item
        android:bottom="2dp"
>
        <shape>
            <solid android:color="#ffff"/>
            <corners android:radius="320dp" />
        </shape>
    </item>
</layer-list>